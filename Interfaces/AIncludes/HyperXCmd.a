	IF &TYPE('__INCLUDINGHYPERXCMD__') = 'UNDEFINED' THEN
__INCLUDINGHYPERXCMD__	SET	1

;  result codes


xresSucc  		EQU 		0
xresFail  		EQU 		1
xresNotImp		EQU 		2

; XCmdBlock Record

XCmdBlock 		RECORD  	0
paramCount		DS.W		1
params			DS.L		16
returnValue   	DS.L		1
passFlag  		DS.B		1
  				DS.B		1
entryPoint		DS.L		1
request   		DS.W		1
result			DS.W		1
inArgs			DS.L		8
outArgs   		DS.L		4
xCmdBlockEnd  	EQU 		*
  				ENDR

	ENDIF	; ...already included 