POPUP	PROC	EXPORT
		IMPORT	REALPOPUP
		BRA.S	@0
		DC.W	0				; flags
		DC.L	('CDEF')		
		DC.W	63				; procID
		DC.W	0				; version
		
@0		JMP		REALPOPUP
		ENDPROC
		
		END